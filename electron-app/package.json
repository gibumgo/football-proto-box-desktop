{
  "name": "electron-app",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:electron": "tsc -p tsconfig.electron.json",
    "start:electron": "npm run build && npm run build:electron && electron .",
    "dev:electron": "concurrently -k \"npm run dev\" \"npm run build:electron -- --watch\" \"wait-on tcp:5173 && electron .\""
  },
  "dependencies": {
    "ag-grid-community": "^35.0.0",
    "ag-grid-react": "^35.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.4.0"
  }
}